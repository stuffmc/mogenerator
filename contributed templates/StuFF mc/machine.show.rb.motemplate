	  <td style="padding-right: 25px;"><b><%= <$RelationshipSingle.name.initialCapitalString$>.human_name %>:</b></td>
	  <td class="padding"><%=h @<$managedObjectClassName.underscorize$>.<$RelationshipSingle.name.underscorize$>.name rescue nil %></td>
  	</tr>
	<$endif$><$endforeach do$>
	
  	</table>
	
	<$foreach Relationship noninheritedRelationships do$><$if Relationship.isToMany$>
